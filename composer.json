{
    "name": "rosamarsky/laravel-doctrine-odm",
    "version": "3.0.1",
    "description": "Simple Laravel Doctrine ODM adapter",
    "keywords": [
        "laravel",
        "doctrine",
        "mongodb",
        "odm",
        "doctrine odm"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Samarsky",
            "email": "rosamarsky@gmail.com",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Rosamarsky\\LaravelDoctrineOdm\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "ext-mongodb": "*",
        "illuminate/container": "*",
        "illuminate/support": "*",
        "nesbot/carbon": "^2|^3",
        "doctrine/mongodb-odm": "^3.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Rosamarsky\\LaravelDoctrineOdm\\": "src"
        }
    }
}
